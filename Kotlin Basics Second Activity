fun main(){
    val age=5*365
    val name = "Rover"
    println("Happy Birthday, ${name}!")
    //let's print a cake
    println("   ,,,,,   ")
    println("   |||||   ")
    println(" =========")
    println("@@@@@@@@@@@")
    println("{~@~@~@~@~}")
    println("@@@@@@@@@@@")
    //This prints an empty line
    println("")
    println("You are already ${age} days old, ${name}!")
    println("${age} days old is the very best age to celebrate")
}
